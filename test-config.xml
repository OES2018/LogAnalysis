<?xml version="1.0" encoding="UTF-8" ?>
<log-analysis>

    <analysis-worker>
        <script-exec>D:/lab/Perl/bin/perl.exe</script-exec>
        <script-file>D:/mydoc/work/loganalisys/loganalysis/script/loganalisys.pl</script-file>

        <thread-pool-size>20</thread-pool-size>
    </analysis-worker>

    <profile name="resin">
        <log-format>%host %other %other %time1 %methodurl %code %cost %bytesd %otherquot %otherquot</log-format>
        <log-separator> </log-separator>
        <log-costunit>us</log-costunit>
    </profile>

    <profile name="apache">
        <log-format>%host %other %other %time1 %methodurl %code %cost %bytesd %otherquot %otherquot</log-format>
        <log-separator> </log-separator>
