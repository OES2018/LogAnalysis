<?xml version="1.0" encoding="UTF-8" ?>
<log-analysis>

    <analysis-worker>
        <script-exec>/usr/bin/perl</script-exec>
        <script-file>/home/webadmin/loganalysis/script/loganalisys.pl</script-file>
        <thread-pool-size>20</thread-pool-size>
    </analysis-worker>

    <profile name="resin">
        <log-format>%host %other %other %time1 %methodurl %code %cost %bytesd %otherquot %otherquot</log-format>
        <log-separator> </log-separator>
        <log-costunit>us</log-costunit>
    </profile>

    <profile name="apache">
        <log-format>%host %other %other %time1 %methodurl %code %cost %bytesd %otherquot %otherquot</log-format>
        <log-separator> </log-separator>
        <log-costunit>us</log-costunit>
    </profile>

    <profile name="nginx1">
        <log-format>%host %other %other %time1 %methodurl %code %bytesd %refererquot %otherquot %otherquot%otherquot %otherquot %otherquot %otherquot %otherquot %costquot</log-format>
        <log-separator> </log-separator>
        <log-costunit>s</log-costunit>
    </profile>

    <profile name="nginx2">
        <log-format>%host %other %other %time1 %methodurl %code %bytesd %refererquot %otherquot %otherquot%otherquot %otherquot%otherquot %otherquot %otherquot %costquot</log-format>
        <log-separator> </log-separator>
        <log-costunit>s</log-costunit>
    </profile>
    
    <log-config name="nginx" profile="nginx2" parent-path="/home/nginxlog">
        <log file="'154_113/protect_acc_log_'yyyy-MM-dd" err-file="" memo="protect"/>
        <log file="'154_138/member_acc_log_'yyyy-MM-dd" err-file="" memo="memeber"/>

        <log file="'154_145/host.access.log_'yyyyMMdd" err-file="" memo="wgapi通用对接_154_145"/>

        <!--畅游论坛-->
        <log file="'154_51/bbs.access.log.'yyyy-MM-dd" err-file="" memo="畅游论坛_154_51"/>
        <log file="'55_35/bbs.access.log.'yyyyMMdd" err-file="" memo="畅游论坛_55_35"/>
        <log file="'64_220/bbs.access.log.'yyyy-MM-dd" err-file="" memo="畅游论坛_64_220"/>

        <!--peak-->
        <log file="'17_55/'yyyyMMdd'.access_log'" err-file="" memo="peak_17_55"/>
        <log file="'17_75/'yyyyMMdd'.access_log'" err-file="" memo="peak_17_75"/>
        <log file="'64_180/'yyyyMMdd'.access_log'" err-file="" memo="peak_64_180"/>

        <!--鹿鼎记论坛-->
